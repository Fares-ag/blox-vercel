{
  "name": "blox-react",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "dev": "npm run dev:admin",
    "dev:unified": "vite",
    "dev:admin": "npm run dev --workspace=packages/admin",
    "dev:customer": "npm run dev --workspace=packages/customer",
    "dev:super-admin": "npm run dev --workspace=packages/super-admin",
    "build": "npm run build --workspaces",
    "build:admin": "npm run build --workspace=packages/admin",
    "build:customer": "npm run build --workspace=packages/customer",
    "build:super-admin": "npm run build --workspace=packages/super-admin",
    "lint": "npm run lint --workspaces",
    "lint:admin": "npm run lint --workspace=packages/admin",
    "lint:customer": "npm run lint --workspace=packages/customer",
    "lint:super-admin": "npm run lint --workspace=packages/super-admin",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:run": "vitest run",
    "preview": "vite preview"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@hookform/resolvers": "^5.2.2",
